package pkg10;

import java.util.Hashtable;

public class HashTable {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Hashtable<Object,Object>map=new Hashtable();
		
		map.put(1,"Mumbai");
		map.put(1,"satara");
		map.put("pune","Mumbai");
		map.put(15,"56");
		map.put(3,"Goa");
		map.put(4,"Banglore");
		
		System.out.println(map);
	}

}
